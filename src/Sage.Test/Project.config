			<locale name="uk" dictionaryNames="uk,en" resourceNames="uk,en,default">
				<format culture="en-uk" shortDate="MMMM d, yyyy" longDate="D"/>
			</locale>
			<locale name="us" dictionaryNames="us,en" resourceNames="us,en,default">
