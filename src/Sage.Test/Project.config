		defaultLocale="com"
		defaultCategory="default"
		sharedCategory="shared"
		multiCategory="true"
		mergeResources="false">

		<paths>
			<ResourcePath>~/assets/{category}</ResourcePath>
		</paths>
		
		<categories>
			<category name="default" locales="ae,ar,at,au,be,br,ca,cf,ch,cn,com,de,dk,es,fi,fr,gr,hk,hu,id,in,it,jp,kr,la,my,nl,no,nz,ph,pl,pt,ru,se,sg,th,tw,uk,us,vn,za"/>
