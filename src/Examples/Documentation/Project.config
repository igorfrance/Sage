<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns="http://www.cycle99.com/schemas/sage/configuration/project.xsd">

	<project
		autoGlobalize="true"
		defaultLocale="en">

		<libraries>
			<library name="global">
				<resources>
					<resource type="icon" location="head" path="/favicon.ico"/>
					<resource type="style" location="head" path="{assetpath}/css/documentation.css"/>
					<resource type="style" location="head" path="{assetpath}/css/documentation-ie.css" limitTo="ie"/>
					<resource type="script" location="head" path="kelp://{assetpath}/scripts/lib/aeon/aeon.js"/>
					<resource type="document" location="data" path="{assetpath}/configuration/navigation.xml"/>
				</resources>
				<include>
					<always/>
				</include>
			</library>
		</libraries>

		<globalization>
			<locale name="en" dictionaryNames="en" resourceNames="default">
				<format culture="en-us" shortDate="MMMM d, yyyy" longDate="D"/>
			</locale>
			<locale name="nl" dictionaryNames="nl,en" resourceNames="nl,default">
				<format culture="nl-nl" shortDate="MMMM d, yyyy" longDate="D"/>
			</locale>
		</globalization>

		<links>
			<link name="documentation" url="documentation/index.html"/>
			<link name="sage-license" url="documentation/sage-license.html"/>
			<link name="sage-obtaining" url="documentation/sage-obtaining.html"/>
