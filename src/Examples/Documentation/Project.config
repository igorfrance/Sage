			<link name="configuration-category" url="documentation/configuration-category.html"/>
			<link name="extension-devtools" url="documentation/extension-devtools.html"/>
		</links>

		<modules>
			<module name="BreadCrumb" category="documentation">
				<stylesheets>
					<stylesheet path="breadcrumb.xsl"/>
				</stylesheets>
				<resources>
					<resource type="script" path="breadcrumb.js" location="body"/>
					<resource type="style" path="breadcrumb.css" location="head"/>
				</resources>
			</module>
			<module name="Navigation" category="documentation">
				<stylesheets>
					<stylesheet path="navigation.xsl"/>
				</stylesheets>
				<resources>
					<resource type="script" path="navigation.js" location="body"/>
					<resource type="style" path="navigation.css" location="head"/>
				</resources>
			</module>
			<module name="DocumentationBoxes" category="documentation">
				<stylesheets>
					<stylesheet path="DocumentationBoxes.xsl"/>
				</stylesheets>
				<resources>
					<resource type="style" path="DocumentationBoxes.css" location="head"/>
				</resources>
			</module>
			<module name="RelatedArticles" category="documentation">
				<stylesheets>
					<stylesheet path="RelatedArticles.xsl"/>
				</stylesheets>
				<resources>
					<resource type="style" path="RelatedArticles.css" location="head"/>
				</resources>
			</module>
			<module name="PageIndex" category="documentation">
				<stylesheets>
					<stylesheet path="PageIndex.xsl"/>
				</stylesheets>
				<resources>
					<resource type="style" path="PageIndex.css" location="head"/>
				</resources>
			</module>
		</modules>

		<developers>
			<ip address="*"/>
		</developers>

	</project>

</configuration>
