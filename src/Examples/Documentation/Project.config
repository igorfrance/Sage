			<link name="sage-modules-example1" url="documentation/sage-modules-example1.html"/>
			<link name="sage-modules-example2" url="documentation/sage-modules-example2.html"/>
			<link name="xslt-extensions-io" url="documentation/xslt-extensions-io.html"/>
			<link name="xslt-extensions-basic" url="documentation/xslt-extensions-basic.html"/>
			<link name="xslt-extensions-set" url="documentation/xslt-extensions-set.html"/>
			<link name="xslt-extensions-string" url="documentation/xslt-extensions-string.html"/>
			<link name="linking" url="documentation/linking.html"/>
			<link name="kelp" url="documentation/kelp.html"/>
			<link name="internationalization" url="documentation/internationalization.html"/>
			<link name="internationalization-fallback" url="documentation/internationalization-fallback.html"/>
